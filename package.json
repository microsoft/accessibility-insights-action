        "license-check-and-add": "^4.0.2",
        "mockdate": "^3.0.5",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.6",
        "ts-loader": "^9.2.2",
        "typemoq": "^2.1.0",
