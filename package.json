        "@types/marked": "^2.0.3",
        "@types/marked-terminal": "^3.1.1",
        "@types/normalize-path": "^3.0.0",
        "@types/puppeteer-core": "^5.4.0",
        "@types/serve-static": "^1.13.9",
        "@types/verror": "^1.10.4",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
