        "eslint": "^8.26.0",
        "eslint-plugin-security": "^1.5.0",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "jest": "^29.2.2",
        "jest-extended": "^3.1.0",
        "jest-file-snapshot": "^0.5.0",
        "jest-junit": "^14.0.1",
